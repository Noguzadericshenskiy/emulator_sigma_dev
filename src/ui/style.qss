#sensor52_lbl, #sensor53_lbl, #sensor54_lbl, #sensor55_lbl, #sensor56_lbl,
#sensor57_lbl, #sensor58_lbl, #sensor59_lbl, #sensor60_lbl, #sensor61_lbl,
