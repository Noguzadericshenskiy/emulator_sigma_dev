QPushButton#norma_btn {
    background-color: rgb(157, 242, 160);
	border: none;

 	font: 75 10pt "Georgia";
 	color: rgb(0, 0, 0);
 	border-radius: 6px;
}

QPushButton#norma_btn:hover {
 	background-color: rgb(152, 234, 154);
 	border-left: 2px solid rgb(0, 160, 0);
 	border-top: 2px solid rgb(0, 160, 0);
 	border-right: 2px solid rgb(0, 160, 0);
 	border-bottom: 2px solid rgb(0, 160, 0);
}

QPushButton#norma_btn:pressed {
 	background-color: rgb(157, 242, 160);
 	background-color: rgb(143, 220, 146);
 	border-left: 2px solid rgb(0, 130, 0);
 	border-top: 2px solid rgb(0, 130, 0);
 	border-right: 2px solid rgb(0, 130, 0);
 	border-bottom: 2px solid rgb(0, 130, 0);
}


QPushButton#fire_btn, #alarm_btn {
	background-color: rgb(173, 0, 0);
	font: 75 10pt "Georgia";
	color: rgb(0, 0, 0);
	border-radius: 6px;
}

QPushButton#fire_btn:hover, #alarm_btn:hover {
	background-color: rgb(173, 0, 0);;
	border-left: 2px solid rgb(0, 160, 0);
	border-top: 2px solid rgb(0, 160, 0);
	border-right: 2px solid rgb(0, 160, 0);
	border-bottom: 2px solid rgb(0, 160, 0);
}

QPushButton#fire_btn:pressed, #alarm_btn:hover {
	background-color: rgb(173, 0, 0);
	border-left: 2px solid rgb(0, 130, 0);
	border-top: 2px solid rgb(0, 130, 0);
	border-right: 2px solid rgb(0, 130, 0);
	border-bottom: 2px solid rgb(0, 130, 0);
}

QPushButton#kz_btn, #break_btn, #non_calibration_btn,
#dirt_btn, #sensitivity_btn, #diff_fire_btn {
	background-color: rgb(255, 140, 0);
	font: 75 10pt "Georgia";
	color: rgb(0, 0, 0);
	border-radius: 6px;
}

QPushButton#kz_btn:hover, #break_btn:hover, #non_calibration_btn:hover,
#dirt_btn:hover, #sensitivity_btn:hover, #diff_fire_btn:hover {
	background-color: rgb(255, 140, 0);
	border-left: 2px solid rgb(0, 160, 0);
	border-top: 2px solid rgb(0, 160, 0);
	border-right: 2px solid rgb(0, 160, 0);
	border-bottom: 2px solid rgb(0, 160, 0);
}

QPushButton#kz_btn:pressed, #break_btn:pressed, #non_calibration_btn:pressed,
#dirt_btn:pressed, #sensitivity_btn:pressed, #diff_fire_btn:pressed {
	background-color: rgb(255, 140, 0);
	border-left: 2px solid rgb(0, 130, 0);
	border-top: 2px solid rgb(0, 130, 0);
	border-right: 2px solid rgb(0, 130, 0);
	border-bottom: 2px solid rgb(0, 130, 0);
}
